<%- include('../partials/header'); %>
    <!-- Container Start -->
    <div class="page-wrapper">
        <div class="main-content">
            <!-- Page Title Start -->
            <div class="row">
                <div class="col xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="page-title-wrapper">
                        <div class="page-title-box">
                            <h4 class="page-title"><%=viewTitle%></h4>
                        </div>
                        <div class="breadcrumb-list">
                            <ul>
                                <li class="breadcrumb-link">
                                    <a href="index.html"><i class="fas fa-home mr-2"></i>Dashboard</a>
                                </li>
                                <li class="breadcrumb-link active"><%=viewTitle%></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Products view Start -->
            <div class="row">
                <!-- Styled Table Card-->
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="card table-card">
                        <div class="card-header ttl-with-btn pb-0">
                            <h4><%=viewTitle%></h4>
                            <!--div class="card-header-btn">
                                <a class="pc-btn resetModalForm" data-toggle="modal" data-target="#createFaqModal" data-whatever="@mdo" id="addNewFaqBtn">Add New FAQ</a>
                            </div-->
                        </div>
                        <div class="card-body">
                           <table class="table table-striped table-bordered dt-responsive nowra display dataTableAjax" style="width:100%" data-action-url="support/question_list" data-target-section="tbody" data-column-class="[{className:'sr'},{className:'cust_question'},{className: 'answer'},{className: 'status'},{className: 'action'}]"  data-refresh-dataTablePosition='0' data-filter='filter' data-sort="[{'targets': [],'orderable': false},{'targets': [],'visible': false}]">
                                <thead>
                                    <tr>
                                        <th class="sr">#</th>
                                        <th class="cust_question">Customer Question</th>
                                        <th class="answer">Answer</th>
                                        <th class="status">Status</th>
                                        <th class="action">Action</th>
                                    </tr>
                                </thead>
                                <tbody>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>

    <!-- Model 6-7-8 -->
	<div class="modal fade" id="questionReplyModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="exampleModalLabel02" data-title="Question Reply">Question Reply </h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <form action="support/question_reply" method="post" autocomplete="off" data-reset="1">
            <div class="modal-body">
                <input type="hidden" name="id" id="que_id" value="0">
                <div class="form-group">
                  <label class="col-form-label">Question:</label>
                  <textarea class="form-control require" name="cust_question" id="cust_question" readonly></textarea>
                </div>
                <div class="form-group">
                  <label class="col-form-label">Answer:</label>
                  <textarea class="form-control" name="cust_answer" id="cust_answer"></textarea>
                </div>
              
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
              <button type="submit" data-action="submitMe" class="btn btn-primary">Submit</button>
            </div>
        </form>
          </div>
        </div>
      </div>
<%- include('../partials/footer'); %>