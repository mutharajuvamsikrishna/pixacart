 
 <%- include('../partials/header'); %>
 <!-- Container Start -->
 <div class="page-wrapper">
    <div class="main-content">
        <!-- Page Title Start -->
        <div class="row">
            <div class="col xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="page-title-wrapper">
                    <div class="page-title-box">
                        <h4 class="page-title"><%=viewTitle%></h4>
                    </div>
                    <div class="breadcrumb-list">
                        <ul>
                            <li class="breadcrumb-link">
                                <a href="index.html"><i class="fas fa-home mr-2"></i>Dashboard</a>
                            </li>
                            <li class="breadcrumb-link active"><%=viewTitle%></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <!-- Products view Start -->
        
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card table-card">
                    <div class="card-header pb-0">
                        <h4><%=viewTitle%></h4>
                    </div>
                    <div class="card-body">
                        <form action="save_settings" method="post" autocomplete="off" data-redirect="settings/privacy-policy">
                            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                <div class="row">
                                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                        <div class="form-group">
                                            <textarea name="privacy_policy_page" class="myEditor"  id="editor_id" placeholder="Enter text ...">
                                                <%= privacyPolicyPage ? JSON.parse(privacyPolicyPage.value) : '' %>
                                            </textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group mb-0">
                                    <!--button class="btn btn-primary" type="button">Reset</button-->
                                    <button class="btn btn-danger" type="submit" id="addPrivacyPolicy" data-action="submitMe">Submit</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <%- include('../partials/footer'); %>