                <div class="row">
                    <div class="col-xl-12">
                        <div class="card-header">
                            <h4>Web Config</h4>
                        </div>
                      <form class="card" action="save_settings" method="post" autocomplete="off" data-redirect="settings/web-config">
                        <div class="card-body">
                          <div class="row">
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">App Name</label>
                                <input class="form-control require" type="text" name="webSetting[APP_NAME]" placeholder="Enter Name" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.APP_NAME : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Contact Person</label>
                                <input class="form-control" type="text" name="webSetting[cp_name]" placeholder="Enter Name" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.cp_name : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Address</label>
                                <input class="form-control" type="text" name="webSetting[address]" name="webSetting[company_name]" placeholder="Enter address" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.address : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Country</label>
                                <input class="form-control" type="text" name="webSetting[country]" placeholder="Country" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.country : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">City</label>
                                <input class="form-control" type="text"  name="webSetting[city]" placeholder="City" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.city : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">State</label>
                                <input class="form-control" type="text"  name="webSetting[state]" placeholder="State" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.state : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Postal Code</label>
                                <input class="form-control" type="text" name="webSetting[postal_code]" placeholder="ZIP Code" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.postal_code : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Email</label>
                                <input class="form-control" type="email" name="webSetting[company_email]" placeholder="Enter Email" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.company_email : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Phone Number</label>
                                <input class="form-control" type="text" name="webSetting[company_phone]" placeholder="Enter Number" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.company_phone : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Mobile Number</label>
                                <input class="form-control" type="text" name="webSetting[company_mobile]" placeholder="Enter Number" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.company_mobile : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Fax</label>
                                <input class="form-control" type="text" name="webSetting[company_fax]" placeholder="818 22 0011" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.company_fax : '' %>">
                              </div>
                            </div>
                            <div class="col-sm-6 col-md-4">
                              <div class="mb-3">
                                <label class="form-label">Website Url</label>
                                <input class="form-control" type="text" name="webSetting[company_url]" placeholder="Enter Url" data-bs-original-title="" title="" value="<%= webSetting ? webSetting.company_url : '' %>">
                              </div>
                            </div>
                          </div>
                          <button class="btn btn-primary squer-btn" data-action="submitMe"  type="submit" data-bs-original-title="" title="">save</button>
                        </div>
                      </form>
                    </div>
                </div>
<%- include('../partials/footer'); %>