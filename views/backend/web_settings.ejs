                                                <% if(webImages && webImages.logo2){ %>
                                                <li class="imgArea">
                                                    <div class="product-upload-image text-center">
                                                      <img src="/uploads/webimages/<%= webImages ? webImages.logo2 : '' %>" alt="image" class="img-fluid">
                                                    </div>
                                                </li>
                                                <% } %>
                                            </ul>  
                                          </div>
                                        </div>
                                      </div>
                                      <div class="form-group mb-0">
                                          <button class="btn btn-primary squer-btn mt-2 mr-2" type="submit" data-action="submitMe">Save</button>
                                      </div>
                                  </form>
                              </div>
                          </div>
                      </div>
                      <div class="col-xl-3 col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <h4>Login Page Image</h4>
                            </div>
                            <div class="card-body">
                                <form action="settings/uploadWebImages" method="post" autocomplete="off" data-redirect="settings/web-config">
                                    <div class="row">
                                      <div class="col-md-12">
                                        <div class="product-upload-wrap">
                                          <ul class="product-upload-list 1-colum">
                                              <li>
                                                  <div>
                                                      <input type="hidden" name="type" value="auth_page_image">
                                                      <input class="form-control d-none require" type="file" placeholder="Upload Image" name="webimages" id="auth_page_image" data-error="Please choose an image to upload.">
                                                      <label class="dragdrop_box" for="auth_page_image">                                                 
                                                          <span class="dragdrop_icon">
                                                              <img src="/images/drag.svg" alt="icon" class="img-fluid">
                                                          </span>
                                                          <div class="dragdrop_info">
                                                              <p><span>Browse</span> Image</p>
                                                              <span class="dragdrop_sprt">Recommended Size 398x374</span><br>
