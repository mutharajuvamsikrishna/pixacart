<%- include('../partials/header'); %>
<!-- Container Start -->
        <div class="page-wrapper">
            <div class="main-content">
                <!-- Page Title Start -->
                <div class="row">
                    <div class="col xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="page-title-wrapper">
                            <div class="page-title-box">
                                <h4 class="page-title"><%=viewTitle%></h4>
                            </div>
                            <div class="breadcrumb-list">
                                <ul>
                                    <li class="breadcrumb-link">
                                        <a href="index.html"><i class="fas fa-home mr-2"></i>Dashboard</a>
                                    </li>
                                    <li class="breadcrumb-link active"><%=viewTitle%></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Products view Start -->
                <div class="row">
                  <!-- Styled Table Card-->
                     <div class="col-xl-6 col-lg-6">
                          <div class="card">
                              <div class="card-header">
                                  <h4>Commission On Sales</h4>
                              </div>
                              <div class="card-body">
                                  <form action="save_settings" method="post" autocomplete="off" data-redirect="settings/web-config">
                                       <div class="row">
                                          <div class="col-xl-12 col-lg-6">
                                              <div class="form-group">
                                                  <label for="text-input" class="col-form-label">Sales Commission (In percent)</label>
                                                  <input class="form-control require" type="number" name="sales_commission"  placeholder="Sales Commission" value="<%= commission ? commission.salesCommission : 0 %>" min="0">
                                              </div>
                                          </div>
                                      </div>
                                      <div class="form-group mb-0">
                                          <button class="btn btn-primary squer-btn mt-2 mr-2" type="submit" data-action="submitMe">Save</button>
                                      </div>
                                  </form>
                              </div>
                          </div>
                      </div>
                      <div class="col-xl-6 col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <h4>Order Cancellation Charges</h4>
                            </div>
                            <div class="card-body">
                                <form action="save_settings" method="post" autocomplete="off" data-redirect="settings/web-config">
                                     <div class="row">
                                        <div class="col-xl-12 col-lg-6">
                                            <div class="form-group">
                                                <label for="text-input" class="col-form-label">Order Cancellation Charges (In percent)</label>
                                                <input class="form-control require" type="number" name="cancellation_charges"  placeholder="Sales Commission" value="<%= commission ? commission.cancellationCharges : 0 %>" min="0">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group mb-0">
                                        <button class="btn btn-primary squer-btn mt-2 mr-2" type="submit" data-action="submitMe">Save</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>   
                <div class="row">
                  <!-- Styled Table Card-->
                     <div class="col-xl-3 col-lg-6">
                          <div class="card">
                              <div class="card-header">
                                  <h4>Login Page Logo</h4>
                              </div>
                              <div class="card-body">
                                  <form action="settings/uploadWebImages" method="post" autocomplete="off" data-redirect="settings/web-config">
                                      <div class="row">
                                        <div class="col-md-12">
                                          <div class="product-upload-wrap">
                                            <ul class="product-upload-list 1-colum">
                                                <li>
                                                    <div>
                                                        <input type="hidden" name="type" value="logo1">
                                                        <input class="form-control d-none require" type="file" placeholder="Upload Image" name="webimages" id="logo_image" data-error="Please choose an image to upload.">
                                                        <label class="dragdrop_box" for="logo_image">                                                 
                                                            <span class="dragdrop_icon">
                                                                <img src="/images/drag.svg" alt="icon" class="img-fluid">
                                                            </span>
                                                            <div class="dragdrop_info">
                                                                <p><span>Browse</span> Image</p>
                                                                <span class="dragdrop_sprt">Recommended Size 38x41</span><br>
                                                                <span class="dragdrop_sprt">Supports: JPG, PNG </span>
                                                                <p><span class="dragdrop_sprt numberOfFiles"></span></p>
                                                            </div>                                                
                                                        </label>
                                                    </div>
                                                </li>
                                                <% if(webImages && webImages.logo1){ %>
                                                <li class="imgArea">
                                                    <div class="product-upload-image text-center">
                                                      <img src="/uploads/webimages/<%= webImages ? webImages.logo1 : '' %>" alt="image" class="img-fluid">
                                                    </div>
                                                </li>
                                                <% } %>
                                            </ul>  
                                          </div>
                                        </div>
                                      </div>
                                      <div class="form-group mb-0">
                                          <button class="btn btn-primary squer-btn mt-2 mr-2" type="submit" data-action="submitMe">Save</button>
                                      </div>
                                  </form>
                              </div>
                          </div>
                      </div>
                      <div class="col-xl-3 col-lg-6">
                        <div class="card">
                            <div class="card-header">
                                <h4>Favicon Image</h4>
