<%- include('../partials/header'); %>
    <!-- Container Start -->
    <div class="page-wrapper">
        <div class="main-content">
            <!-- Page Title Start -->
            <div class="row">
                <div class="col xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="page-title-wrapper">
                        <div class="page-title-box">
                            <h4 class="page-title"><%=viewTitle%></h4>
                        </div>
                        <div class="breadcrumb-list">
                            <ul>
                                <li class="breadcrumb-link">
                                    <a href="index.html"><i class="fas fa-home mr-2"></i>Dashboard</a>
                                </li>
                                <li class="breadcrumb-link active"><%=viewTitle%></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Products view Start -->
            <div class="row">
                <!-- Styled Table Card-->
                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                    <div class="card table-card">
                        <div class="card-header ttl-with-btn pb-0">
                            <h4><%=pageTitle%></h4>
                        </div>
                        
                        <div class="card-body">
                            <input type="hidden" name="seller_id" class="filter" value="<%=seller_id;%>">
                           <table class="table table-striped table-bordered dt-responsive nowrap display dataTableAjax" style="width:100%" data-action-url="orders/Transactions_list" data-target-section="tbody" data-column-class="[{className:'sr'},{className:'order_id'},{className:'c_name'},{className:'debit'},{className:'credit'},{className:'balance'},{className: 'entry_againts'},{className: 'remark'},{className: 'createdAt'}]"  data-refresh-dataTablePosition='0' data-filter='filter' data-sort="[{'targets': [],'orderable': false},{'targets': [],'visible': false}]">
                                <thead>
                                    <tr>
                                        <th class="sr">#</th>
                                        <th class="order_id">Order Id</th>
                                        <th class="c_name">Customer Name</th>
                                        <th class="debit">Debit</th>
                                        <th class="credit">Credit</th>
                                        <th class="balance">Balance</th>
                                        <th class="entry_againts">Entry Againts</th>
                                        <th class="remark">Remark</th>
                                        <th class="createdAt">Date</th>
                                    </tr>
                                </thead>
                                <tbody>

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>   
<%- include('../partials/footer'); %>